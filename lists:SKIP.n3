@prefix : <urn:example:> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

# Extra tests when rdf:first and rdf:rest are built-ins...

{
    # This is true in cases where rdf:rest is a built-in on list-terms
    (1 2 3) rdf:rest (2 3) . #
}
=> 
{
    :test1 a :Result .
} .

{
    # If previous works, should then also this one ?
    (1 2 3) rdf:rest [ rdf:first 2 ; rdf:rest [ rdf:first 3 ; rdf:rest rdf:nil ] ] .
}
=>
{
    :test2 a :Result .
} .

{
    # Then also this one should give a result ?
2
    _:x0 rdf:first 2 ;
         rdf:rest _:x1 .

    _:x1 rdf:first 3 ;
         rdf:rest rdf:nil .

    (1 2 3) rdf:rest _:x0 .
}
=>
{
    :test3 a :Result .
} .
 
{
    # And also this one ?

    (1 2 3) rdf:rest _:x0 .

    _:x0 rdf:first 2 ;
         rdf:rest _:x1 .

    _:x1 rdf:first 3 ;
         rdf:rest rdf:nil .
}
=>
{
    :test4 a :Result .
} .

{
    # And also this one ?

    _:a0 rdf:first 1 ;
         rdf:rest _:a1 .

    _:a1 rdf:first 2 ;
         rdf:rest _:a2 .
   
    _:a2 rdf:first 3 ;
         rdf:rest rdf:nil .

    ####################
    _:a0 rdf:rest _:x0 .
    ####################

    _:x0 rdf:first 2 ;
         rdf:rest _:x1 .

    _:x1 rdf:first 3 ;
         rdf:rest rdf:nil .
}
=>
{
    :test5 a :Result .
} .
