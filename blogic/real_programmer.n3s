@prefix : <urn:example:> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

############################################## 
# Test 1

# Bob 
:Bob :eats :Quiche.

# This should fail
#:Bob a :RealProgrammer .

#
##############################################

############################################## 
# Test 2

# Alice doesn't eat Quiche
() log:onNegativeSurface {
    :Alice :eats :Quiche .
} .

# This should fail
#:Alice :eats :Quiche .

#
##############################################

############################################## 
# Test 3

() log:onNegativeSurface {
    :Charly a :RealProgrammer .
} .

#
##############################################

############################################## 
# Test 4

() log:onNegativeSurface {
    () log:onNegativeSurface {
        :Dave :eats :Quiche .
    } .
} .

() log:onNegativeSurface {
    () log:onNegativeSurface {
        () log:onNegativeSurface {
            :Estelle a :RealProgrammer.
        } .
    } .
} .

#
##############################################

# An exclusive disjunction P <-/-> Q means that P is true or Q but not both
# This is same as
#     P <-> ~Q
# and is the same as
#     ( P -> ~Q ) AND (~Q -> P) AND ( ~P -> Q ) AND ( Q -> ~ P )
# P = real programmer
# Q = eat quiche
(_S) log:onNegativeSurface {

    _S a :RealProgrammer .

    () log:onNegativeSurface {
    
        () log:onNegativeSurface {
            _S :eats :Quiche  .
        } .
    } .
}.

(_S) log:onNegativeSurface {

    () log:onNegativeSurface {
        _S a :RealProgrammer .
    } .

    () log:onNegativeSurface {
        _S :eats :Quiche .
    } .
}.

(_S) log:onNegativeSurface {

    _S :eats :Quiche .
 
    () log:onNegativeSurface {
        () log:onNegativeSurface {
            _S a :RealProgrammer .
        } .
    } .
}.

# Q (eat quiche) -> P ( you are a programmer)
#
# Q -> P = ~ ( Q AND ~P )
(_S) log:onNegativeSurface {
    _S :eats :Quiche .

    () log:onNegativeSurface {
        _S a :Programmer .
    } .
} .

(_S _C) log:onQuerySurface {
    _S a _C.
} .

(_S _C) log:onQuerySurface {
    _S :eats _C .
} .

(_S _C) log:onQuerySurface {
    () log:onNegativeSurface { _S :eats _C } .
} .

(_S _C) log:onQuerySurface {
    () log:onNegativeSurface { _S a _C } .
} .

# Test
() log:onNegativeSurface {
    :Alice a :RealProgrammer .
    :Bob a :Programmer .
    :Charly a :Programmer .
    :Dave a :Programmer .
    :Estelle a :Programmer .
    () log:onNegativeSurface {
        :test :is true .
    } .
} .

() log:onQuerySurface {
    :test :is true .
} .