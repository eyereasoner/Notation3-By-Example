@prefix : <urn:example:> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

:Alice a :Person , :Cyclist .
() log:onNegativeSurface {
    :Alice a :IceSkater .
} .

# All persons are cyclist and iceskater
(?X) log:onNegativeSurface {
    ?X a :Person .

    () log:onNegativeSurface {
        ?X a :Cyclist , :IceSkater .
    } .
} .