@prefix : <urn:example:> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

() log:onNegativeSurface {
    :A a :B .
    () log:onNegativeSurface {
        :C a :D .
    } .
} .

() log:onNegativeSurface {
    :A a :B .
    :C a :D .
} .

(_S _O) log:onNegativeSurface {
    () log:onNegativeSurface {
        _S a _O .
    } .

    () log:onNegativeSurface {
        :test :is true .
    } .
} .

# Test
() log:onQuerySurface {
    :test :is true .
} .
