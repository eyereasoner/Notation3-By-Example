@prefix : <urn:example:> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

_:x a :Cat .
:Minka a :Cat .

(_:x) log:onNegativeSurface {
    _:x a :Cat .
    () log:onNegativeSurface {
        _:x :says "meow"@en .
    } .
} .

() log:onQuestionSurface {
    _:x :says "meow"@en .
    :Minka :says "meow"@en .
    () log:onAnswerSurface {
        :test :is true .
    } .
} .
