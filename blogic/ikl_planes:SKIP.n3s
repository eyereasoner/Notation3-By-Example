@prefix : <urn:example:> .
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

# TPTP format
#
# fof(p3,axiom,(
#    ? [X,Y,Z] :
#      ( type(X,plane) &
#        type(Y,plane) & 
#        type(Z,plane) & 
#        id(X,1) & id(Y,2) & id(Z,3) &
#        X!=Y & X!=Z & Y!=Z 
#      )
# )).
#
# ## There is no plane with id(1) and id(2) where both are the same
# fof(test,conjecture,(
#    ? [X,Y] : ~( type(X,plane) & type(Y,plane) & id(X,1) & id(Y,2) & X=Y)
# )).
#

_A1 a :Airplane .
_A2 a :Airplane .
_A3 a :Airplane .

_A1 :id 1 .
_A2 :id 2 .
_A3 :id 3 .

_A1 owl:differentFrom ?A2 .
_A1 owl:differentFrom ?A3 .
_A2 owl:differentFrom ?A3 .

