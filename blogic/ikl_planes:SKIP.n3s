() log:onNegativeSurface {
    # So this must be true
    (?X ?Y) log:onNegativeSurface {
        ?X a :Airplane .
        ?Y a :Airplane .
        ?X :id 1 .
        ?Y :id 2 .
        ?X owl:sameAs ?Y .
    } .
    () log:onNegativeSurface {
        :test :is true .
    }
} .

() log:onQuerySurface {
    :test :is true .
} .

# OWL

(?A ?B) log:onNegativeSurface {
    ?A owl:differentFrom ?B .
    () log:onNegativeSurface {
        ?B owl:differentFrom ?A .
    } . 
} . 

(?A ?B ?C) log:onNegativeSurface {
    ?A owl:sameAs ?B .
    ?A owl:differentFrom ?B .
} .  

(?A ?B ?C ?D) log:onNegativeSurface {
    ?A owl:sameAs ?B .
    ?A ?C ?D .
    () log:onNegativeSurface {
        ?B ?C ?D .
    } .
} .
    
(?A ?B ?C ?D) log:onNegativeSurface {
    ?A owl:sameAs ?B .
    ?C ?A ?D .
    () log:onNegativeSurface {
        ?C ?B ?D .
    } .
} . 

(?A ?B ?C ?D) log:onNegativeSurface {
    ?A owl:sameAs ?B .
    ?D ?C ?A .
    () log:onNegativeSurface {
        ?D ?C ?B .
    } .
} .

(?A ?B) log:onNegativeSurface {
    ?A owl:sameAs ?B .
    () log:onNegativeSurface {
        ?B owl:sameAs ?A .
    } .
} .

(?A ?B ?C) log:onNegativeSurface {
    ?A owl:sameAs ?B .
    ?B owl:sameAs ?C .
    () log:onNegativeSurface {
        ?A owl:sameAs ?C .
    } .
} .
