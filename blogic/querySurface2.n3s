@prefix : <urn:example:> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .
@prefix list: <http://www.w3.org/2000/10/swap/list#> .

:Alice a :Person .

# Create a dynamic query surface
(?X ?Y) :is { ?X a ?Y }.
(?X ?Y) :is { ?X :is ?Y }.

(?P ?Q) log:onNegativeSurface {
    ?P :is ?Q .

    () log:onNegativeSurface {
        ?P log:onQuerySurface ?Q .
        # Make test happy
        :test :is true .
    } .
} .
