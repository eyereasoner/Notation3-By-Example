@prefix : <urn:example:> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .
@prefix list: <http://www.w3.org/2000/10/swap/list#> .

:Alice a :Person .

# Create a dynamic query surface
(_X _Y) :is { _X a _Y }.
(_X _Y) :is { _X :is _Y }.

(_P _Q) log:onNegativeSurface {
    _P :is _Q .

    () log:onNegativeSurface {
        _P log:onQuerySurface _Q .
        # Make test happy
        :test :is true .
    } .
} .
