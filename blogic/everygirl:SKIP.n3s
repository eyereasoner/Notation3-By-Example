@prefix : <urn:example:> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

:Alice a :Girl .

# \Every x : \Some y : x a Girl & y a Boy & y loves x
(_X) log:onNegativeSurface {
   (_Y) log:onNegativeSurface {
        _X a :Girl .
        _Y a :Boy .
        _Y :loves _X .
   } . 
} .

(_X _Y) log:onQuestionSurface {
    _X :loves _Y .
    () log:onAnswerSurface {
        :test :is true .
   } .
} .
