@prefix : <urn:example:> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

:Alice a :Girl .

# \Every x : \Some y : x a Girl & y a Boy & y loves x
(?X) log:onNegativeSurface {
   (?Y) log:onNegativeSurface {
        ?X a :Girl .
        ?Y a :Boy .
        ?Y :loves ?X .
   } . 
} .

(?X ?Y) log:onQuestionSurface {
    ?X :loves ?Y .
    () log:onAnswerSurface {
        :test :is true .
   } .
} .
