@prefix : <urn:example:> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

:Alice a :Cyclist .
() log:onNegativeSurface {
    :Alice a :IceSkater .
} .

# All cyclists are iceskaters and all iceskaters are cyclists
(_X) log:onNegativeSurface {
    _X a :Cyclist .
    () log:onNegativeSurface {
        _X a :IceSkater .
    } .
} . 

(_X) log:onNegativeSurface {
    _X a :IceSkater .
    () log:onNegativeSurface {
        _X a :Cyclist .
    } .
} . 