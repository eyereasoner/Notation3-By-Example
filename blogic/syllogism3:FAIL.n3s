@prefix : <urn:example:> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

:Alice a :Cyclist .
() log:onNegativeSurface {
    :Alice a :IceSkater .
} .

# All cyclists are iceskaters and all iceskaters are cyclists
(?X) log:onNegativeSurface {
    ?X a :Cyclist .
    () log:onNegativeSurface {
        ?X a :IceSkater .
    } .
} . 

(?X) log:onNegativeSurface {
    ?X a :IceSkater .
    () log:onNegativeSurface {
        ?X a :Cyclist .
    } .
} . 