@prefix : <urn:example:> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

:alice a :person .
:alice :likes :cat .

() log:onQuestionSurface {
    () log:onNegativeSurface {
      (_X) log:onNegativeSurface {
         :alice :likes _X .
      } .
    } .
    () log:onAnswerSurface {
        :test :is true .
    } .
} .