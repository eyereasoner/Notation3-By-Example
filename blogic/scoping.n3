@prefix : <urn:example.org:> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

?x a :Cat .
:Minka a :Cat .

(?x) log:onNegativeSurface {
    ?x a :Cat .
    () log:onNegativeSurface {
        ?x :says "meow"@en .
    } .
} .

() log:onQuestionSurface {
    ?x :says "meow"@en .
    :Minka :says "meow"@en .
    () log:onAnswerSurface {
        :test :is true .
    } .
} .
