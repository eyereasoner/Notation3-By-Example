@prefix : <https://example.org/ns/>.
@prefix log: <http://www.w3.org/2000/10/swap/log#> .



# Print output result to main surface
(_:G) log:onQuerySurface {
    () log:onResultSurface _:G.
    () log:onQuerySurface _:G.
} .

# Log warnings separately
(_:S _:P _:O) log:onQuerySurface {
    () log:onWarningSurface {
        _:S _:P _:O .    
    } .
} .
