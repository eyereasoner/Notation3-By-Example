@prefix : <http://example.org/ns#> .
@prefix query: <https://github.com/phochste/query#> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

() log:onNeutralSurface {
    :MyQuery a query:Query ;
        query:select {
            (?S ?O) 
                query:where {
                    ?S a ?O ;
                        :likes :Fruit .
                    # ?S ?P ?O # fails
                } ;
                query:result {
                    ?S a ?O .
                } .
        } .
}.
