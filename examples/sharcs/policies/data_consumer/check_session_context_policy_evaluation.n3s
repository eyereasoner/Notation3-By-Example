@prefix : <http://www.example.org/rdf#> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

# ForAll Login, User, Membership, Subscription, Permission, Allows:
# (<Login> a Login) &
# (<Login> user <User>) &
# (<Login> membership <Membership>) &
# (<Permission> a Permission) &
# (<Permission> isActive true) &
# (<Permission> hasMembership <Membership>) &
# (<Permission> allows <Allows>) &
# (<Allows> subscription <Subscription>) &
# (<Allows> subscriber <User>)
# IMPLIES:
#   (<Login> permission <Permission>) &
#   (<Login> a ValidPermission)
(_Login _User _Membership _Subscription _Permission _Allows) log:onNegativeSurface {
    _Login 
        a :Login ;
        :user _User ;
        :membership _Membership ;
        :subscription _Subscription .

    _Permission
        a :Permission;
        :isActive true;
        :hasMembership _Membership .

    _Permission
        :allows _Allows.

    _Allows 
        :subscription _Subscription ;
        :subscriber _User .

    () log:onNegativeSurface {
        _Login :permission _Permission .
        _Login a :ValidPermission .
    } .
} .
