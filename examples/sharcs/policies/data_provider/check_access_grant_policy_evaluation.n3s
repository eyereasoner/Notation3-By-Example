@prefix : <http://www.example.org/rdf#> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

# ForAll Login, User, Client, AccessGrant :
# (<Login> a Login) &
# (<Login> as <User>) &
# (<Session> user <User>) &
# (<Session> status LoggedIn) &
# (<Session> client <Client>) &
# (<AccessGrant> a AccessGrant) &
# (<AccessGrant> isActive true) & 
# (<AccessGrant> client <Client>) 
# IMPLIES
#    (<Login> a ValidAccessGrant)
(_:Login _:User _:Session _:Client _:AccessGrant) log:onNegativeSurface {
    _:Login 
        a :Login ;
        :as _:User .

    _:Session 
        a :LoginSession ;
        :user _:User ;
        :status :LoggedIn ;
        :client _:Client .

    _:AccessGrant
        a :AccessGrant ;
        :isActive true ;
        :client _:Client .
    
    () log:onNegativeSurface {
        _:Login
            a :ValidAccessGrant ;
            :grant _:AccessGrant .
    }
} .
