@prefix : <http://www.example.org/rdf#> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

# ForAll Login, User, Client, AccessGrant :
# (<Login> a Login) &
# (<Login> as <User>) &
# (<Session> user <User>) &
# (<Session> status LoggedIn) &
# (<Session> client <Client>) &
# (<AccessGrant> a AccessGrant) &
# (<AccessGrant> isActive true) & 
# (<AccessGrant> client <Client>) 
# IMPLIES
#    (<Login> a ValidAccessGrant)
(_Login _User _Session _Client _AccessGrant) log:onNegativeSurface {
    _Login 
        a :Login ;
        :as _User .

    _Session 
        a :LoginSession ;
        :user _User ;
        :status :LoggedIn ;
        :client _Client .

    _AccessGrant
        a :AccessGrant ;
        :isActive true ;
        :client _Client .
    
    () log:onNegativeSurface {
        _Login
            a :ValidAccessGrant ;
            :grant _AccessGrant .
    }
} .
