@prefix : <http://www.example.org/rdf#> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

# ForAll Login, User, Client, AccessGrant :
# (<Login> a Login) &
# (<Login> as <User>) &
# (<Session> user <User>) &
# (<Session> status LoggedIn) &
# (<Session> client <Client>) &
# (<AccessGrant> a AccessGrant) &
# (<AccessGrant> isActive true) & 
# (<AccessGrant> client <Client>) 
# IMPLIES
#    (<Login> a ValidAccessGrant)
(?Login ?User ?Session ?Client ?AccessGrant) log:onNegativeSurface {
    ?Login 
        a :Login ;
        :as ?User .

    ?Session 
        a :LoginSession ;
        :user ?User ;
        :status :LoggedIn ;
        :client ?Client .

    ?AccessGrant
        a :AccessGrant ;
        :isActive true ;
        :client ?Client .
    
    () log:onNegativeSurface {
        ?Login
            a :ValidAccessGrant ;
            :grant ?AccessGrant .
    }
} .
