@prefix : <http://www.example.org/rdf#>.
@prefix log: <http://www.w3.org/2000/10/swap/log#>.

# WHEN:
# (<Login> a Login) &&
# (<Login> as <User>) &&
# (<Login> isValidLogin true) &&
# (<Login> isValidMembership true) &&
# (<Login> isValidOrganization true) &&
# (<Login> isValidSubscription true) &&
# (<Login> isValidPermission true) 
# THEN:
#   (<Login> a ValidAll)
(_:Login) log:onNegativeSurface {
    _:Login 
        a :Login ;
        a :ValidLogin ;
        a :ValidMembership ;
        a :ValidOrganization ;
        a :ValidSubscription ;
        a :ValidPermission .

    () log:onNegativeSurface {
        _:Login a :ValidAll .
    } .
} .

