@prefix : <http://www.example.org/rdf#>.
@prefix log: <http://www.w3.org/2000/10/swap/log#>.

# WHEN:
# (<Login> a Login) &&
# (<Login> as <User>) &&
# (<Login> isValidLogin true) &&
# (<Login> isValidMembership true) &&
# (<Login> isValidOrganization true) &&
# (<Login> isValidSubscription true) &&
# (<Login> isValidPermission true) 
# THEN:
#   (<Login> isValid true)
(_:Login) log:onNegativeSurface {
    _:Login 
        a :Login ;
        :isValidLogin true ;
        :isValidMembership true ;
        :isValidOrganization true ;
        :isValidSubscription true ;
        :isValidPermission true .

    () log:onNegativeSurface {
        _:Login :isValid true .
    } .
} .

