@prefix : <urn:example.org:> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

:Alice a :Human .
:Alice :likes :Cats , :Dogs .

(?S ?What) log:onNegativeSurface {
   ?S :likes :Cats .
   ?What log:equalTo :Cats .
   () log:onNegativeSurface {
        :result :is ?What .
   } .
} .

(?S ?What) log:onNegativeSurface {
   ?S :likes :Dogs .
   ?What log:equalTo :Dogs .
   () log:onNegativeSurface {
        :result :is ?What .
   } .
} .

(?S) log:onNegativeSurface {
    () log:onNegativeSurface <<?S :likesAnimals :Dogs>> .
    ?S :likes :Dogs . 
} .     

(?S ?O) log:onQuerySurface {
    ?S :is ?O .
} .
