@prefix : <urn:example.org:> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

:Alice a :Human .
:Alice :likes :Cats , :Dogs .

(_S _What) log:onNegativeSurface {
   _S :likes :Cats .
   _What log:equalTo :Cats .
   () log:onNegativeSurface {
        :result :is _What .
   } .
} .

(_S _What) log:onNegativeSurface {
   _S :likes :Dogs .
   _What log:equalTo :Dogs .
   () log:onNegativeSurface {
        :result :is _What .
   } .
} .

(_S) log:onNegativeSurface {
    () log:onNegativeSurface <<_S :likesAnimals :Dogs>> .
    _S :likes :Dogs . 
} .     

(_S _O) log:onQuerySurface {
    _S :is _O .
} .
