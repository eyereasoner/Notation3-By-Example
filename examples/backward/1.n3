@prefix : <urn:example.org:> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

:Alice a :Human .
:Alice :likes :Cats .

(?S) log:onNegativeSurface {
   ?S :likesAnimals true .
   () log:onNegativeSurface {
        :result :is true .
   } .
} .

(?S) log:onNegativeSurface {
    () log:onNegativeSurface <<?S :likesAnimals true>> .
    ?S :likes :Cats . 
} .     

(?S ?O) log:onQuerySurface {
    ?S :is ?O .
} .
