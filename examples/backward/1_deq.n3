@prefix : <urn:example.org:> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

:Alice a :Human .
:Alice :likes :Cats .

($ _:S $) log:onNegativeSurface {
   _:S :likes :Cats .

   ($ $) log:onNegativeSurface {
        :result :is true .
   } .
} .

($ _:S _:O $) log:onQuerySurface {
    _:S :is _:O .
} .
