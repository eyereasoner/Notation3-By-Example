@prefix : <urn:example.org:> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .
@prefix list: <http://www.w3.org/2000/10/swap/list#> .

:Alice a :Human .
:Alice :likes (:Cats :Dogs) .

(?S ?What ?L) log:onNegativeSurface {
   ?S :likes ?L .
   () log:onNegativeSurface {
        (?L () ) :reverse_ ?What .
   } .
} .

(?Acc ?What) log:onNegativeSurface {
    ( () ?Acc ) :reverse_ ?What .
    () log:onNegativeSurface {
        :result :is ?Acc .
    } .
} .

(?A ?B ?Acc ?AccN ?H ?T) log:onNegativeSurface {
    ( ?A ?Acc ) :reverse_ ?B .
    ?A list:firstRest (?H ?T) .
    ((?H) ?Acc) list:append ?AccN .
    () log:onNegativeSurface {
        ( ?T ?AccN) :reverse_ ?B .
    } . 
} .

(?S ?O) log:onQuerySurface {
    ?S :is ?O .
} .
