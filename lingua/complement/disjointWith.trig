@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix lingua: <http://www.w3.org/2000/10/swap/lingua#>.
@prefix var: <http://www.w3.org/2000/10/swap/var#>.
@prefix : <#>.

# Definition of disjointWith


# DisjointWith is symmetric
_:implication1
    lingua:premise _:pre1 ;
    lingua:conclusion _:con1 .

_:pre1 log:closedBy <> .
_:con1 log:closedBy <> .

_:pre1 {
    var:X log:disjointWith var:Y .
}

_:con1 {
    var:Y log:disjoinWith var:X .
}

# DisjointWith parts can't be equal
_:implication2 
    lingua:premise _:pre2 ;
    lingua:conclusion false .

_:pre2 log:closedBy <> .

_:pre2 {
    var:X log:disjointWith var:Y .
    var:X log:equalTo var:Y .
}


